<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="560" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
		<!--  fx:controller = "LoginPageController"-->
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="username" layoutX="376.0" layoutY="189.0" PromptText = "Username"/>
            <TextField fx:id="password" layoutX="376.0" layoutY="255.0" />
            <Button fx:id="CreateAcctButton" layoutX="376.0" layoutY="316.0" mnemonicParsing="false" onAction="#createUserPageClick" text="New User" />
            <Button fx:id="loginButton" layoutX="479.0" layoutY="316.0" mnemonicParsing="false" onAction="#loginClick" text="Login" />
            <Label layoutX="317.0" layoutY="188.0" text="User">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="318.0" layoutY="254.0" text="Pass">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
