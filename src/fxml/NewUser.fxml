<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="560" prefWidth="900" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
		<!--fx:controller = "LoginPageController"-->
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
		<Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f8c325" height="34.0" stroke="TRANSPARENT" strokeType="INSIDE" width="900.0" />
		<Label layoutX="14.0" layoutY="9.0" text="User Name/Password">
			<font>
				<Font name="Arial Black" size="12.0" />
			</font>
		</Label>
		<Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f8c325" height="34.0" layoutY="131.0" stroke="TRANSPARENT" strokeType="INSIDE" width="900.0" />
		<Label layoutX="14.0" layoutY="139.0" text="User Stats">
			<font>
				<Font name="Arial Black" size="12.0" />
			</font>
		</Label>
		<Label layoutX="94.0" layoutY="57.0" text="User Name:">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>
		<Label layoutX="121.0" layoutY="94.0" text="e-Mail:">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>
		<Label layoutX="426.0" layoutY="94.0" text="Re-type Password:">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>
		<Label layoutX="480.0" layoutY="57.0" text="Password:">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>
		<Label layoutX="104.0" layoutY="173.0" text="Gender:">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>
		<Label layoutX="127.0" layoutY="218.0" text="Age:">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>
		<Label layoutX="450.0" layoutY="173.0" text="Height (inches):">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>
		<Label layoutX="488.0" layoutY="218.0" text="Weight:">
			<font>
				<Font name="Arial Bold" size="12.0" />
			</font>
		</Label>

		<TextField fx:id="usernameField" layoutX="162.0" layoutY="52.0" prefHeight="12" prefWidth="200">
		</TextField>
		<TextField fx:id="emailField" layoutX="161.0" layoutY="89.0" prefHeight="12" prefWidth="200">
		</TextField>
		<TextField fx:id="passwordField" layoutX="543.0" layoutY="52.0" prefHeight="12" prefWidth="200">
		</TextField>
		<TextField fx:id="passwordConfirmField" layoutX="543.0" layoutY="89.0" prefHeight="12" prefWidth="200">
		</TextField>
		<ComboBox fx:id="genderBox" layoutX="161.0" layoutY="167.0" prefWidth="200" />
		<TextField fx:id="ageField" layoutX="161.0" layoutY="213.0" prefHeight="12" prefWidth="200">
		</TextField>
		<TextField fx:id="heightField" layoutX="543.0" layoutY="167.0" prefHeight="12" prefWidth="200">
		</TextField>
		<TextField fx:id="weightField" layoutX="543.0" layoutY="212.0" prefHeight="12" prefWidth="200">
		</TextField>
      
		<Button fx:id="create" layoutX="581.0" layoutY="280.0" mnemonicParsing="false" onAction="#createClick" text="Create New Account" />
		<Button fx:id="back" layoutX="241.0" layoutY="280.0" mnemonicParsing="false" onAction="#backClick" text="Back" />
      </children>
    </AnchorPane>
  </children>
</VBox>
